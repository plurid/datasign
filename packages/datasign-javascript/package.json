{
    "name": "@plurid/datasign",
    "version": "0.0.0-0",
    "description": "Single Source of Truth Data Contract Specifier",
    "keywords": [
        "datasign"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/datasign"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/datasign/issues"
    },
    "homepage": "https://github.com/plurid/datasign/blob/master/packages/datasign-javascript/README.md",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/index.d.ts",
    "bin": {
        "datasign": "binder/datasign"
    },
    "files": [
        "distribution",
        "binder"
    ],
    "preferGlobal": false,
    "scripts": {
        "test": "jest -c ./configurations/jest.config.js ./source",
        "test.suite": "jest -c ./configurations/jest.config.js",
        "watch": "rollup -c ./scripts/rollup.config.js -w",
        "build.cleanup": "find ./distribution -type d -name '*__tests__' -exec rm -rf {} + && find ./distribution -type f -name '*.DS_Store' -exec rm -rf {} +",
        "build": "rollup -c ./scripts/rollup.config.js",
        "prepublishOnly": "yarn build && yarn build.cleanup"
    },
    "dependencies": {
        "commander": "^6.2.0",
        "cross-fetch": "^3.0.6",
        "sync-fetch": "^0.3.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@types/jest": "^26.0.15",
        "@types/node": "^14.14.10",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.14.0",
        "jest": "^26.6.3",
        "rollup": "^2.34.0",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-typescript2": "^0.29.0",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.2"
    }
}
